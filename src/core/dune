(library
  (name containers)
  (public_name containers)
  (wrapped false)
  (flags :standard -w +a-4-42-44-48-50-58-32-60@8 -safe-string -nolabels -open CCMonomorphic)
  (ocamlopt_flags (:include ../flambda.flags))
  (libraries containers.monomorphic)
  )
